package experiment;

public class BoardCell {

	private int row, column;

	public BoardCell(int row, int column) {
		super();
		this.row = row;
		this.column = column;
	}

	public int getRow() {
		return row;
	}

	public int getColumn() {
		return column;
	}

	@Override
	public String toString() {
		return "[" + row + "][" + column + "]";
	}
	
	

}
